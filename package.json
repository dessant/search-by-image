    "@material/theme": "^4.0.0",
    "@material/typography": "^4.0.0",
    "buffer": "^6.0.3",
    "core-js": "^3.16.0",
    "cropperjs": "^1.5.12",
    "ext-components": "github:dessant/ext-components#v0.4.1",
    "ext-contribute": "github:dessant/ext-contribute#v0.3.5",
    "file-type": "16.5.3",
    "idb-keyval": "^5.1.3",
    "imagesloaded": "^4.1.4",
    "lodash-es": "^4.17.21",
    "masonry-layout": "^4.2.2",
    "p-queue": "^7.1.0",
    "spinkit": "^1.2.5",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-resize": "^1.0.1",
    "vuedraggable": "^2.24.3",
    "webextension-polyfill": "^0.8.0",
    "wesa": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.7",
    "del": "^6.0.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-jsonmin": "^1.2.0",
    "gulp-merge-json": "^2.1.1",
    "gulp-postcss": "^9.0.0",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-check-updates": "^11.8.3",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "recursive-readdir": "^2.2.2",
    "sass": "^1.37.2",
    "sass-loader": "^12.1.0",
    "sharp": "^0.28.3",
    "standard-version": "^9.3.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "web-ext": "^6.2.0",
    "webpack": "^5.48.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2"
  },
  "private": true
}
